{
  "name": "@z-scraper/crypto-api",
  "version": "0.0.1",
  "description": "A TypeScript/JavaScript SDK for the Crypto API that lets you fetch aggregated crypto news from multiple sources and AI-powered sentiment analysis through RapidAPI with minimal setup.",
  "keywords": [
    "crypto",
    "crypto-api",
    "sdk",
    "typescript",
    "javascript",
    "rapidapi",
    "news",
    "sentiment"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-scraper/crypto-api.git"
  },
  "bugs": {
    "url": "https://github.com/z-scraper/crypto-api/issues"
  },
  "homepage": "https://github.com/z-scraper/crypto-api#readme",
  "license": "MIT",
  "author": "Z (z-scraper)",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepare": "npm run build",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/nock": "^10.0.3",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "nock": "^14.0.10",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
